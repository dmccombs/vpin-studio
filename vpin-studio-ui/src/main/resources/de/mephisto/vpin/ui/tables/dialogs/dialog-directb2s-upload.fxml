<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<BorderPane styleClass="base-component" stylesheets="@../../stylesheet.css" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.mephisto.vpin.ui.tables.dialogs.DirectB2SUploadController">
   <top>
      <fx:include fx:id="header" source="/de/mephisto/vpin/commons/fx/scene-dialog-header.fxml" />
   </top>
   <center>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
         <top>
            <BorderPane BorderPane.alignment="CENTER">
               <center>
                  <HBox>
                     <children>
                        <TextField fx:id="fileNameField" prefHeight="26.0" prefWidth="375.0">
                           <HBox.margin>
                              <Insets right="12.0" />
                           </HBox.margin>
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                        <Button mnemonicParsing="false" onAction="#onFileSelect" prefHeight="26.0" prefWidth="99.0" text="Select File...">
                           <font>
                              <Font size="14.0" />
                           </font>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin></Button>
                     </children>
                  </HBox>
               </center>
               <top>
                  <Label fx:id="titleLabel" text="DirectB2S File:" textFill="WHITE" BorderPane.alignment="TOP_LEFT">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <BorderPane.margin>
                        <Insets bottom="6.0" />
                     </BorderPane.margin>
                  </Label>
               </top>
               <BorderPane.margin>
                  <Insets bottom="24.0" left="24.0" right="24.0" top="24.0" />
               </BorderPane.margin>
            </BorderPane>
         </top>
         <bottom>
            <HBox alignment="BASELINE_RIGHT" BorderPane.alignment="CENTER">
               <children>
                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onCancelClick" styleClass="default-button" text="Cancel">
                     <HBox.margin>
                        <Insets right="12.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="uploadBtn" layoutX="418.0" layoutY="10.0" mnemonicParsing="false" onAction="#onUploadClick" styleClass="submit-button" text="Upload">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
               </children>
               <BorderPane.margin>
                  <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
               </BorderPane.margin>
            </HBox>
         </bottom>
         <left>
            <VBox BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets left="24.0" top="6.0" />
               </BorderPane.margin>
               <children>
                  <RadioButton fx:id="uploadTypeTable" mnemonicParsing="false" selected="true" text="Upload to &quot;Tables&quot; folder" textFill="WHITE">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <toggleGroup>
                        <ToggleGroup fx:id="group" />
                     </toggleGroup>
                  </RadioButton>
                  <RadioButton fx:id="uploadTypeGenerator" mnemonicParsing="false" text="Upload as background image for the card generator and competitions only" textFill="WHITE" toggleGroup="$group">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="6.0" />
                     </VBox.margin>
                  </RadioButton>
               </children>
            </VBox>
         </left>
      </BorderPane>
   </center>
</BorderPane>
